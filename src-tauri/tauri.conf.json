{
	"productName": "Tauri App",
	"version": "0.1.0",
	"identifier": "com.tauri.dev",
	"build": {
		"frontendDist": "./frontend",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "python3 -m http.server 5173",
		"beforeBuildCommand": "npm run build"
	},
	"app": {
		"windows": [
			{
				"title": "Tauri",
				"width": 800,
				"url": "index.html",
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
